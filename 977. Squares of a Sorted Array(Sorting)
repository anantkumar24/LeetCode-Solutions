class Solution {
public:
    vector<int> sortedSquares(vector<int>& nums) {
        int n=nums.size();
        vector<int>ans;
        for(int i=0;i<n;i++)
        {
            int squ=nums[i]*nums[i];
            ans.push_back(squ);
        }
        sort(ans.begin(),ans.end());
        return ans;
        
    }
};
