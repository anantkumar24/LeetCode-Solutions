class Solution {
public:
    int maximumPopulation(vector<vector<int>>& logs) {
        vector<int>a(101,0);
        for(int i=0;i<logs.size();i++)
        {
            int birth=logs[i][0];
            int death=logs[i][1];
            a[birth-1950]++;
            a[death-1950]--;
        }
        int maxyear=1950,currentpop=0,maxpop=0;
        for(int i=0;i<101;i++)
        {
            currentpop+=a[i];
            if(currentpop>maxpop)
            {
                maxpop=currentpop;
                maxyear=1950+i;
            }
        }
        return maxyear;

        
    }
};
